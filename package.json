{
    "name": "kodus-mcp-manager",
    "version": "0.0.1",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "build": "nest build",
        "start": "nest start",
        "start:dev": "nest start --watch --debug=0.0.0.0:9140",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "docker:build": "docker-compose -f docker-compose.yml up --build",
        "docker:start": "yarn docker:down && yarn docker:up",
        "docker:up": "docker-compose -f docker-compose.yml up",
        "docker:down": "docker-compose -f docker-compose.yml down",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "sh ./test/run-e2e.sh",
        "openapi:export": "ts-node ./scripts/openapi/export-openapi.ts",
        "openapi:lint": "ts-node ./scripts/openapi/lint-openapi.ts",
        "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli",
        "migration:generate": "f() { yarn typeorm migration:generate -p -d ./src/config/typeorm.config.ts \"./src/database/migrations/$@\"; }; f",
        "migration:run": "yarn typeorm migration:run -d ./src/config/typeorm.config.ts",
        "migration:revert": "yarn typeorm migration:revert -d ./src/config/typeorm.config.ts",
        "pre:migrate": "./scripts/create-schema.sh",
        "migrate": "yarn pre:migrate && docker-compose exec kodus-mcp-manager npm run migration:run"
    },
    "dependencies": {
        "@composio/core": "^0.1.36-next.2",
        "@fastify/cors": "^11.2.0",
        "@fastify/swagger": "^9.6.1",
        "@fastify/swagger-ui": "^5.2.5",
        "@kodus/flow": "^0.1.40",
        "@nestjs/common": "^11.1.13",
        "@nestjs/config": "^4.0.3",
        "@nestjs/core": "^11.1.13",
        "@nestjs/jwt": "^11.0.2",
        "@nestjs/platform-fastify": "^11.1.13",
        "@nestjs/swagger": "^11.2.6",
        "@nestjs/typeorm": "^11.0.0",
        "@opentelemetry/api": "^1.9.0",
        "axios": "^1.13.4",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.3",
        "dotenv": "^17.2.3",
        "pg": "^8.18.0",
        "reflect-metadata": "^0.2.2",
        "rxjs": "^7.8.2",
        "typeorm": "^0.3.28"
    },
    "devDependencies": {
        "@nestjs/cli": "^11.0.16",
        "@nestjs/schematics": "^11.0.9",
        "@nestjs/testing": "^11.1.13",
        "@stoplight/spectral-cli": "^6.15.0",
        "@types/config": "^3.3.5",
        "@types/express": "^5.0.6",
        "@types/jest": "^30.0.0",
        "@types/node": "^25.2.1",
        "@types/supertest": "^6.0.3",
        "@typescript-eslint/eslint-plugin": "^8.54.0",
        "@typescript-eslint/parser": "^8.54.0",
        "eslint": "^9.39.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.5",
        "jest": "^30.2.0",
        "pino-pretty": "^13.1.3",
        "prettier": "^3.8.1",
        "source-map-support": "^0.5.21",
        "supertest": "^7.2.2",
        "ts-jest": "^29.4.6",
        "ts-loader": "^9.5.4",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.9.3"
    },
    "resolutions": {
        "chalk": "^4.1.2"
    }
}
